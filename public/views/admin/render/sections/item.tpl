<tr>
	<td>{{title}}</td>
	<td>{{filename}}.tpl</td>
	<td>{% if navigation %}<i class="fa fa-check"></i>{% endif %}</td>
	<td class="center">
		<div class="buttons nowrap notop">
			<button sectionedit="{{id}}" sectiontitle="{{title}}"><i class="fa fa-edit"></i></button>
			<button sectionremove="{{id}}" sectiontitle="{{title}}" class="remove"><i class="fa fa-trash"></i></button>
		</div>
	</td>
</tr>